// code1
<!DOCTYPE html>
<html>
<body>

<h2>JavaScript in Body</h2>

<p id="demo"></p>

<script>
document.getElementById("demo").innerHTML="my namme is khan";
</script>

</body>
</html> 


//code2

<!DOCTYPE html>
<html>
<head>
<script>

function myfunction()
{
document.getElementById("demo").innerHTML="not a terrorist";
}
</script>
</head>

<body>

<h2>JavaScript in Head</h2>

<p id="demo">A Paragraph.</p>

<button type="button" onclick="myfunction()"></button>

</body>
</html> 

//code3. used javascript in body

<!DOCTYPE html>
<html>
<head>

</head>

<body>
<script>

function myfunction()
{
document.getElementById("demo").innerHTML="not a terrorist";
}
</script>
<h2>JavaScript in Head</h2>

<p id="demo">A Paragraph.</p>

<button type="button" onclick="myfunction()"></button>

</body>
</html> 





