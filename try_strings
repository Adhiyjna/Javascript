//concat of strings

<!DOCTYPE html>
<html>
<body>

<p id="demo">Display the result here.</p> 

<script>
var str1 = "Hello ";
var str2 = "World!";

document.getElementById("demo").innerHTML=str1+str2;
</script>

</body>
</html>



