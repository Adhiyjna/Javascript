//starting code

<!DOCTYPE html>
<html>
<body>

<p id="demo">Display the result here.</p> 

<script>
var cars = ["Saab", "Volvo", "BMW"];
document.getElementById("demo").innerHTML = cars;
</script>

</body>
</html>

// indexing using cars

<!DOCTYPE html>
<html>
<body>

<p id="demo"></p> 

<script>
var cars = ["Saab", "Volvo", "BMW"];
document.getElementById("demo").innerHTML = cars[1];
</script>

</body>
</html>

//how to show length of the following
<!DOCTYPE html>
<html>
<body>

<p id="demo"></p> 

<script>
var cars = ["Saab", "Volvo", "BMW"];
document.getElementById("demo").innerHTML = cars.length;
</script>

</body>
</html>
