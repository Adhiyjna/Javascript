<!DOCTYPE html>
<html>
<body>

<p id="demo"></p>

<script>
var text = "";
var i;
for (i = 1; i < 10; i++) {
	if(i==5 || i==7)continue;
    document.getElementById("demo").innerHTML += i + "<br>";
}
</script>

</body>
</html>

//switch case

<!DOCTYPE html>
<html>
<body>

<input id="myInput" type="text" value="BMW">
<button onclick="checkCar()">Check Car</button>
<p id="demo"></p>

<script>
function checkCar() {
  var text;
  var favCar = document.getElementById("myInput").value;

  switch(favCar) {
    case "BMW":
      text = "German car";
      break;      
    case "Ford":
      text = "American car";
      break;      
    case "Peugeot":
      text = "French car";
      break;
    default:
      text = "Unknown car name";
  }
  document.getElementById("demo").innerHTML = text;
}
</script>

</body>
</html>
